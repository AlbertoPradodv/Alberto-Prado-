        (((a.FREQUENCIA - 2.9326448916331858)/1.870102831646055) * 0.556141432808529) as FREQUENCIA_PAD,
        (((a.QTD_VIDEO_START_COMPLETUDE75 - 4.872869841612681)/6.374846350901733) * 0.5263426360208749) as QTD_VIDEO_START_COMPLETUDE75_PAD,
        (((a.DIVERSIDADE_DEVICES - 1.190524489250882)/0.41795078210598674) * 0.22254692077457272) as DIVERSIDADE_DEVICES_PAD,
        ((((a.DIVERSIDADE_TITULOS - 1.635513609517541)/1.2529982549872758) * 0.3008233306584926)) as DIVERSIDADE_TITULOS_PAD
        ,((((a.HOURS_PLAYED - 4.204590985480387)/5.1368135882211545) * 0.556141432808529)+(((a.FREQUENCIA - 2.9326448916331858)/1.870102831646055) * 0.556141432808529)+(((a.QTD_VIDEO_START_COMPLETUDE75 - 4.872869841612681)/6.374846350901733) * 0.5263426360208749)+(((a.DIVERSIDADE_DEVICES - 1.190524489250882)/0.41795078210598674) * 0.22254692077457272)+((((a.DIVERSIDADE_TITULOS - 1.635513609517541)/1.2529982549872758) * 0.3008233306584926))) + 6.2 as IEN
